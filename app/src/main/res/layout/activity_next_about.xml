<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yang.study.ServiceNextActivity">
    <TextView
        android:id="@+id/tv_nextActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nextActivity"
        android:background="@color/color_97B2EE"
        android:padding="3dp"
        android:textSize="22sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/tv_nextBind"/>

    <TextView
        android:id="@+id/tv_nextBind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="nextBindService"
        android:background="@color/color_97B2EE"
        android:padding="3dp"
        android:textSize="22sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_nextActivity"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>